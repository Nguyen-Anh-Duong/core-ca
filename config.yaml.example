keymanagement:
  softhsm:
    module: /usr/lib/softhsm/libsofthsm2.so
    slot: "YOUR_SLOT_ID"
    pin: "YOUR_PIN"
    
ca:
  issuer: "CN=Your CA Name,O=Your Organization,C=VN"
  validity_days: 2920
  database:
    dsn: "postgres://username:password@localhost:5432/core_ca?sslmode=disable"

